<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head %>
</head>
<body>

  <header>
      <% include partials/header %>
  </header>

  <style>
  h3, h5 {
    text-align: center;
  }
  </style>

  <main class = "container">
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
      <div class="well">
        <% if (document).ready(function() { %>
          $('#calendar').fullCalendar({
            googleCalendarApiKey: 'process.env.GOOGLE_API_KEY',
            events: {
              googleCalendarId: 'studs.up.sea@gmail.com'
            }
          });
        <% }); %>
      </div>
    </div>
    <div class="col-xs-0 col-sm-0 col-md-2 col-lg-2">
      
    </div>
    <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
      <div class="well">
        <h3>Meet up</h3>
        <p>Schedule events and meets here!</p>
        <h5><a href="/meet">Meet Up!</a></h5>
      </div>
      
    </div>
  </div>
  </main>

  <% include partials/footer %>
    
</body>
</html>